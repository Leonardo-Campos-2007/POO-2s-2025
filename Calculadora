package calculadora;

public class Calculadora 
{
   public Double numeroA;
   public Double numeroB;
    

   public void calculadoraa()
   {

    Double resultadoSoma1 = this.numeroA + this.numeroB;
    Double resultadoSoma2 = this.numeroA - this.numeroB;
    Double resultadoSoma3 = this.numeroA / this.numeroB;
    Double resultadoSoma4 = this.numeroA * this.numeroB;
   
    


    System.out.println(this.numeroA + " + " + this.numeroB + " = " + resultadoSoma1);
    System.out.println(this.numeroA + " - " + this.numeroB + " = " + resultadoSoma2);
    System.out.println(this.numeroA + " / " + this.numeroB + " = " + resultadoSoma3);
    System.out.println(this.numeroA + " * " + this.numeroB + " = " + resultadoSoma4);

   }

   
}
